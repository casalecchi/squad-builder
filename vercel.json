{
    "builds": [
        { "src": "server/index.js", "use": "@vercel/node" },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ]
}
